{
    "name": "boilerplate-webpack-v3",
    "version": "2.0.0",
    "description": "boilerplate-webpack-v3",
    "author": "zengyou1, zhiyou, jilong5",
    "scripts": {
        "clean": "rimraf assets",
        "clean-test": "rimraf test",
        "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./webpack.config.js --color --progress -p",
        "build-test": "npm run clean-test && cross-env NODE_ENV=developmentTest webpack --config ./webpack.config.js --color --progress -d",
        "build-test-ie": "cross-env NODE_ENV=developmentIE webpack --config ./webpack.config.js --color --progress --watch -d",
        "dev-ie": "http-server ./test-ie -p 80",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --public test.sina.cn --config ./webpack.config.js --port 80",
        "dev2": "node ./server/dev.js"
    },
    "dependencies": {
        "babel-polyfill": "^6.3.14",
        "es5-shim": "^4.5.8",
        "lodash": "^4.17.11",
        "swiper": "^4.2.6"
    },
    "devDependencies": {
        "ajv": "^6.3.0",
        "autoprefixer": "^6.7.7",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-plugin-minify-dead-code-elimination": "^0.1.4",
        "babel-plugin-transform-remove-console": "^6.9.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-0": "^6.3.13",
        "clean-webpack-plugin": "^0.1.14",
        "colors": "^1.2.1",
        "cross-env": "^4.0.0",
        "css-loader": "^0.23.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.11",
        "glob": "^6.0.4",
        "html-loader": "^0.4.5",
        "html-webpack-plugin": "^2.15.0",
        "http-server": "^0.11.1",
        "image-webpack-loader": "^4.2.0",
        "mockjs": "^1.0.1-beta3",
        "node-sass": "^4.9.0",
        "open-browser-webpack-plugin": "^0.0.3",
        "optimist": "^0.6.1",
        "postcss": "^6.0.22",
        "postcss-color-rgba-fallback": "^2.0.0",
        "postcss-loader": "^1.3.3",
        "postcss-opacity": "^4.0.0",
        "precss": "^3.1.2",
        "rimraf": "^2.4.3",
        "sass-loader": "^6.0.7",
        "style-loader": "^0.12.4",
        "tmodjs-loader": "^1.0.1",
        "uglifyjs-webpack-plugin": "^1.2.4",
        "url-loader": "^0.6.2",
        "webpack": "^3.11.0",
        "webpack-dashboard": "^0.2.0",
        "webpack-dev-server": "^2.11.2",
        "webpack-spritesmith": "^0.3.0"
    }
}
